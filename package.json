{
  "name": "beaker-bot",
  "version": "1.0.0",
  "description": "Beaker Workflow god",
  "author": "Martin Styk <mastyk@redhat.com>",
  "license": "GPLv3",
  "repository": "https://github.com//beaker-bot-ng.git",
  "homepage": "https://github.com//beaker-bot-ng",
  "bugs": "https://github.com//beaker-bot-ng/issues",
  "keywords": [
    "github",
    "bot-app"
  ],
  "scripts": {
    "dev": "nodemon",
    "start": "probot run ./index.js",
    "lint": "standard --fix",
    "test": "jest --coverage && standard",
    "test:watch": "jest --watchAll --notify --notifyMode=change --coverage"
  },
  "dependencies": {
    "axios": "^0.19.2",
    "probot": "^7.2.0"
  },
  "devDependencies": {
    "jest": "^24.0.0",
    "nock": "^10.0.0",
    "nodemon": "^1.17.2",
    "smee-client": "^1.1.0",
    "standard": "^12.0.1"
  },
  "engines": {
    "node": ">= 8.3.0"
  },
  "standard": {
    "env": [
      "jest"
    ]
  },
  "nodemonConfig": {
    "exec": "npm start",
    "watch": [
      ".env",
      "."
    ]
  },
  "jest": {
    "testEnvironment": "node"
  }
}
